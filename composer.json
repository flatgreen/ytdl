{
    "name": "flatgreen/ytdl",
    "description": "A PHP wrapper for youtube-dl or yt-dlp tool. Close to the command line, with a caching system.",
    "keywords": ["youtube-dl", "yt-dlp"],
    "license": "MIT",
    "authors": [
        {
            "name": "flatgreen",
            "homepage": "https://github.com/flatgreen"
        }
    ],
    "autoload": {
        "psr-4": {
            "Flatgreen\\Ytdl\\": "src/"
        },
            "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "require": {
        "php": ">=7.4",
        "symfony/process": "^5|^6",
        "psr/log": "^1|^2|^3",
        "symfony/cache": "^5|^6"
    },
    "autoload-dev": {
        "psr-4": {
            "Flatgreen\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "brianhenryie/color-logger": "^1",
        "phpstan/phpstan": "^1.8",
        "monolog/monolog": "^2.7"
    }
}
